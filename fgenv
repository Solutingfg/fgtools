#[ -x /oracle/exploit ] && SCRIPT_DIR=/oracle/exploit
[ -z "${SCRIPT_DIR}" ] && SCRIPT_DIR=${TOOLKIT_BIN:-$(dirname $(which --skip-alias ora_functions.sh &>/dev/null) &>/dev/null)}


